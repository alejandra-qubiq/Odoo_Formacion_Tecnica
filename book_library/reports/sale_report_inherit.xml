<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="sale_report_inherit" inherit_id="sale.report_saleorder_document">
            <xpath expr="//div[@t-if='doc.user_id.name']" position="after">
                <t t-if="doc.partner_id.partner">
                    <strong>Membership number:</strong><br/>
                    <span t-field="doc.partner_id.partner_number"/>
                </t>
            </xpath>
    </template>
</odoo>
